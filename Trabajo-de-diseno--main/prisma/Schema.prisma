datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    email String @unique
    name String String?
    password Sturtl ring
        telephone String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    role Role @default(USER)
}

enum Role {
    USER,
    ADMIN
}